{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-console":0,
      "no-unused-vars":1,
			"no-tabs":0,
      "react/prop-types":0,
      "array-callback-return":0
    }
}
